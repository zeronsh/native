        "expo-splash-screen": "~0.30.10",
        "expo-status-bar": "~2.2.3",
        "expo-web-browser": "^14.2.0",
        "nanoid": "^5.1.5",
        "pg": "^8.16.3",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-native": "0.79.5",
        "react-native-edge-to-edge": "^1.6.0",
