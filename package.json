        "@stardazed/streams-text-encoding": "^1.0.2",
        "@t3-oss/env-core": "^0.13.8",
        "@ungap/structured-clone": "^1.3.0",
        "ai": "5.0.0-beta.21",
        "better-auth": "^1.2.12",
        "drizzle-orm": "^0.44.3",
        "effect": "^3.16.16",
        "expo": "~53.0.17",
        "expo-constants": "^17.1.7",
        "expo-font": "~13.3.2",
        "expo-linking": "~7.1.7",
        "expo-router": "~5.1.3",
